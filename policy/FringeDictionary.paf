#<?cfg paf dictionary ?>

target: Config

definitions: {
    num: {
        type: int
        description: "Number of regions to measure on CCD"
        maxOccurs: 1
        default: 30000
    }
    size: {
        type: int
        description: "Size of region (pixels)"
        maxOccurs: 1
        default: 10
    }
    iterations: {
        type: int
        description: "Number of fitting iterations"
        maxOccurs: 1
        default: 20
    }
    clip: {
        type: double
        description: "Clipping threshold (stdevs)"
        maxOccurs: 1
        default: 2.5
    }
    discard: {
        type: double
        description: "Discard threshold (stdevs)"
        maxOccurs: 1
        default: 5.0
    }
}
