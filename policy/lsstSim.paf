#<?cfg paf policy>

do: {
    bias: true
    flat: true
    defects: true
    interpolate: true
    saturation: true
    overscan: true
    background: true
    cr: true
    phot: true
    ast: true
    cal: true
}

background: {
    binsize: 128
}

bootstrap: {                            # detect bright objects
    thresholdValue: 60.0
}

detect: {
    thresholdValue: 5.0
    thresholdType: stdev
}

measure: @@meas_algorithms:policy:MeasureSourcesDefaults.paf

cr: {
    nCrPixelMax: 200000
}

ast: {
    defaultFilterName: i
    numBrightStars: 100
}
