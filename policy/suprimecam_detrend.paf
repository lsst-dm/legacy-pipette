#<?cfg paf policy>

do: {
    isr: {
        processAmp: {
            saturation: true
            overscan: true
        }
        assembly: true
        bias: false
        variance: true
        dark: false
        flat: false
        fringe: false
        defects: true
        background: false
    }
    cr: true
    phot: true
    scale: NONE
}

detect: {
    thresholdValue: 5.0
    thresholdType: stdev
}

measure: @@meas_algorithms:policy:MeasureSourcesDefaults.paf

cr: {
    nCrPixelMax: 200000
}

